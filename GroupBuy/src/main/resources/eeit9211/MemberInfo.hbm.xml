<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.MemberBean" table="MemberInfo" >
        
        <id name="memberNo" column="member_No">
            <generator class="identity"/>
        </id>
        
        <property name="memberStatus" column="otherStatus" />
        <property name="memberAccount" column="member_Account" />
        <property name="memberName" column="member_Name" />
        <property name="memberPassword" column="member_Password" />
        <property name="memberNickName" column="member_NickName" />
        <property name="memberBirth" column="member_Birth" />
        <property name="memberPhone" column="member_Phone" />
        <property name="memberAddress" column="member_Address" />
        <property name="memberPic" column="member_Pic" />
        <property name="groupAttendanceTotalSuccess" column="groupAttendance_TotalSuccess" />
        <property name="groupAttendanceTotalQt" column="groupAttendance_TotalQt" />
        <property name="groupCredit" column="groupCredit" />
      
        
<!--         <many-to-one name="deptVO" class="com.dept.model.DeptVO" lazy="false"> -->
<!--             <column name="deptno" /> -->
<!--         </many-to-one> -->
        
    </class>
</hibernate-mapping>

